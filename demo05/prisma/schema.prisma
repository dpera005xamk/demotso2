generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite" 
  url      = "file:./data.db"
}

model Ostos {
  id       Int     @id @default(autoincrement()) // perusavaimen määritelmä (@id)
  tuote    String
  poimittu Boolean @default(false)
} // muotoiltu: oikea klick, format document

